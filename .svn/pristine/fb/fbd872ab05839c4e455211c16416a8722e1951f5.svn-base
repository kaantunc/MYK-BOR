	#cboxLoadedContent form#form-login ul li a {
	background:transparent url(../images/default/bullet.png) no-repeat scroll left center;
	padding-left:23px;	}
	
	#modlgn_username, #modlgn_passwd {
		height:17px;
		padding:2px;
		width:100%;}

	
	
