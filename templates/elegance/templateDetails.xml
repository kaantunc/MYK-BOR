		<param name="xml_menuname" type="menu" default="mainmenu" label="Menu Name" description="What menu would you like to use for the default default menu?" />
		<param name="xml_s5_body_width" type="text" default="920" label="Body Width" description="What width would you like for the website?" />
		<param name="xml_s5_left_width" type="text" default="200" label="Left Column Width" description="What width in pixels would you like the left column to be?" />
		<param name="xml_s5_right_width" type="text" default="290" label="Right Column Width" description="What width in pixels would you like the right column to be?" />	
		<param name="xml_s5_mainmodulewidth" type="text" default="600" label="Top Left Width" description="Enter the width for the topleft module position." />
		<param name="xml_s5_insetwidth" type="text" default="200" label="Inset Width" description="Enter the width for the inset module position." />
		<param name="xml_s5_menu" type="list" default="1" label="Menu Type" description="Enter in 1 for No-MooMenu, 2 for Suckerfish, 3 for Fading No-Moo, or 4 for Nothing">
			<option value="1">S5 Drop In No-MooMenu</option>
			<option value="2">Suckerfish</option>
			<option value="3">S5 Fading No-MooMenu</option>
			<option value="4">S5 Scroll Down No-MooMenu</option>
			<option value="5">No Menu</option>
		</param>	
		<param name="xml_s5_color" type="list" default="1" label="Choose your site color" description="Choose your overall site color">
			<option value="1">Light</option>
			<option value="2">Dark</option>
		</param>	
		<param name="xml_s5_login" type="text" default="Login" label="Login Text" description="Enter the text you desire for the Login button." />
		<param name="xml_s5_register" type="text" default="Register" label="Register Text" description="Enter the text you desire for the Register button." />
		<param name="xml_s5_twitter" type="text" default="javascript:;" label="Twitter URL" description="Enter the url you desire for the twitter button." />
		<param name="xml_s5_rss" type="text" default="javascript:;" label="RSS URL" description="Enter the url you desire for the rss button." />
		<param name="xml_s5_backcolor" type="text" default="#ffffff" label="Background Color" description="Enter the URL the background color for your site. Enter transparent for none." />
		<param name="xml_s5_headerback" type="text" default="templates/elegance/images/elegance_gradient.jpg" label="Body Background" description="Enter the URL to a background image that will repeat throughout the body of the template. Leave blank for none." />

		<param name="xml_s5_seourl" type="text" default="" label="Your Site URL if SEO enabled" description="If you turn on SEO URLs and your site loses its CSS formatting and flat out doesn't work enter your full site url here.  Example: http://www.mysite.com" />
		<param name="xml_s5_tooltips" type="text" default="yes" label="Tooltips" description="Enable tooltips" />
		<param name="xml_s5_lytebox" type="list" default="yes" label="Enable Lytebox?" description="Enable the lytebox feature for your images  NOTE: it is highly recommened to disable if you are not planning to use">
			<option value="yes">yes</option>
			<option value="no">no</option>
		</param>	
	
		
		</params>
</install>